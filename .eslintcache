2256-detective/src/reportWebVitals.js",[],"/Users/kellymariewright/Documents/Girl Scouts/troop2256-detective/src/App.js",[],"/Users/kellymariewright/Documents/Girl Scouts/troop2256-detective/src/index.js",[],"/Users/kellymariewright/Documents/Girl Scouts/troop2256-detective/src/Home.js",["33","34","35"],"import React from 'react';\nimport {\n  Col,\n  Row,\n} from 'reactstrap';\nimport {\n  useHistory,\n} from 'react-router-dom';\n\nimport sadSanta from './assets/sad-santa.jpg'\nimport reindeerCookies from './assets/reindeer-cookie.jpg'\nimport detective from './assets/detectives.jpg'\n\nimport Lock from './Lock';\n\nexport default function Home () {\n  const history = useHistory();\n\n  return (\n    <div className=\"home\">\n      <Row>\n        <Col md={4}>\n          <img src={reindeerCookies} />\n        </Col>\n        <Col>\n          <h4 className='row'>\n            Santa always starts delivering presents at Sally's house.\n          </h4>\n          <h4 className='row'>\n            He found many years ago that Girl Scout cookies help his reindeer fly through the night.\n          </h4>\n        </Col>\n      </Row>\n      <br/>\n      <br/>\n      <Row>\n        <Col>\n          <h4 className='row'>\n            This year, when Santa arrived at Sally's house, there was a cold glass of milk, and a plate full of ...\n          </h4>\n          <h4 className='row'>\n            nothing.  The plate was empty\n          </h4>\n          <br/>\n          <h4 className='row'>\n            Santa knows his reindeer depend on these cookies to help them deliver the presents to all the girls and boys.\n          </h4>\n          <h4 className='row'>\n            Without them, Santa fears he many not be able to deliver them all.\n          </h4>\n        </Col>\n        <Col md={3}>\n          <img src={sadSanta} />\n        </Col>\n      </Row>\n      <br/>\n      <br/>\n      <br/>\n      <Row>\n        <Col md={2}>\n          <img src={detective} />\n        </Col>\n        <Col>\n          <h4 className='row'>\n            Santa needs a detective to solve the mystery of his missing Trefoil cookies. \n          </h4>\n          <h4 className='row'>\n            Work with your team to solve the case and save Christmas. \n          </h4>\n        </Col>\n      </Row>\n      <br/>\n      <hr/>\n      <br/>\n      \n      <Lock\n        answer=''\n        question='What team are you on?'\n        clickFunction={() => history.push('/detective')}\n        required={false}\n      />\n    </div>\n  );\n}\n","/Users/kellymariewright/Documents/Girl Scouts/troop2256-detective/src/Lock.js",["36","37"],"import React, { useState, useEffect } from 'react';\nimport {\n  Button,\n  Col,\n  Form,\n  FormGroup,\n  Input,\n  Label,\n  Row,\n} from 'reactstrap';\nimport * as _ from 'lodash';\nimport PropTypes from 'prop-types';\n\nimport lock from './assets/lock.png';\nimport './App.css';\n\nexport default function Lock ({\n  answer,\n  clickFunction,\n  question,\n  required,\n}) {\n  const [lockCode, setLockCode] = useState();\n  const matches = (\n    _.lowerCase(lockCode) === _.lowerCase(answer)\n  ) || (\n    _.isArray(answer) &&\n    _.includes(answer, _.lowerCase(lockCode))\n  );\n\n  return (\n    <div className=\"lock\">\n      <Row>\n        <Col>\n          <Form>\n            <FormGroup>\n              <Label for={question}>\n                {question}\n              </Label>\n              <Input\n                id={question}\n                type='text'\n                onChange={(e) => setLockCode(e.target.value)}\n              />\n            </FormGroup>\n          </Form>\n        </Col>\n        <Col md={3}>\n          <Button\n            className='lock-btn'\n            disabled={required && !matches}\n            onClick={clickFunction}\n          >\n            <img className='lock-img' src={lock} />\n            &nbsp;&nbsp;&nbsp; {\n              !matches && required ?\n              'Locked' :\n              'Unlocked'\n            }\n          </Button>\n        </Col>\n      </Row>\n    </div>\n  );\n}\n\n\nLock.propTypes = {\n  answer: PropTypes.oneOfType([\n    PropTypes.string,\n    PropTypes.array,\n  ]),\n  question: PropTypes.string,\n  required: PropTypes.bool,\n  clickFunction: PropTypes.func,\n};\n\nLock.defaultProps = {\n  required: true,\n  clickFunction: () => {},\n};\n","/Users/kellymariewright/Documents/Girl Scouts/troop2256-detective/src/Detective.js",["38"],"import React, { useState } from 'react';\nimport {\n  Button,\n  Row,\n} from 'reactstrap';\nimport { useHistory } from 'react-router-dom';\n\nimport Lock from './Lock';\n\nexport default function Home () {\n  const history = useHistory();\n\n  const [hasDate, setHasDate] = useState(false);\n  const [hasGirlGuide, setHasGirlGuide] = useState(false);\n  const [hasBadges, setHasBadges] = useState(false);\n  const [hasSign, setHasSign] = useState(false);\n  const [hasMotto, setHasMotto] = useState(false);\n  const [hasCircle, setHasCircle] = useState(false);\n\n  return (\n    <div className=\"detective\">\n      <Row>\n        <h4 className='row'>\n          To be a great detective, you have to start with the basics.\n        </h4>\n        <h4 className='row'>\n          Learn about Girl Scouts to know how you can help solve the mystery.\n        </h4>\n        <br />\n        <iframe \n          width=\"560\" \n          height=\"315\" \n          src=\"https://www.youtube.com/embed/RguQd_1PGDw\" \n          frameBorder=\"0\" \n          allow=\"accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture\" \n          allowFullScreen\n        ></iframe>\n      </Row>\n      <Lock\n        answer='1912'\n        question='What year did Girl Scouts begin?'\n        clickFunction={() => setHasDate(true)}\n      />\n      <br />\n      <hr />\n      <br />\n      <Lock\n        answer={['girl guides', 'girl guide']}\n        question='What were Girl Scouts originally called (and still named in many countries)?'\n        clickFunction={() => setHasGirlGuide(true)}\n      />\n      <br />\n      <hr />\n      <br />\n      {hasDate && hasGirlGuide &&\n        <div>\n          <Row>\n            <h5>\n              Badges\n            </h5>\n          </Row>\n          <Row>\n            <p>\n              A cornerstone of Girl Scouting, the seven legacy badges build on over 100 years of Girl Scout history. Each of these badges (Artist, Athlete, Citizen, Cook, First Aid, Girl Scout Way, and Naturalist) is available at five levels of Girl Scouting, from Brownie to Ambassador.\n            </p>\n          </Row>\n          <Row>\n            <h5>\n              Girl Scout Sign\n            </h5>\n            <p>\n              Girl Scouts make the Girl Scout sign—raising three fingers of the right hand with the thumb holding down the pinky—when they say the Girl Scout Promise. The three fingers represent the three parts of the Promise.\n            </p>\n          </Row>\n          <Row>\n            <h5>\n              Motto\n            </h5>\n          </Row>\n          <Row>\n            <p>\n              The Girl Scout motto is \"Be prepared.\" In the 1947 Girl Scout Handbook, the motto was explained this way: \"A Girl Scout is ready to help out wherever she is needed. Willingness to serve is not enough; you must know how to do the job well, even in an emergency.\" The same holds true today.\n            </p>\n          </Row>\n          <Row>\n            <h5>\n              Friendship Circle\n            </h5>\n          </Row>\n          <Row>\n            <p>\n              Representing the unbroken chain of friendship among Girl Scouts and Girl Guides around the world, the Friendship Circle involves Girl Scouts standing in a circle, crossing their right arms over their left, and clasping hands with their friends on both sides. Everyone then makes a silent wish as a friendship squeeze is passed from hand to hand around the circle.  Many meetings end with this circle.\n            </p>\n          </Row>\n          <Lock\n            answer={['badge', 'badges']}\n            question='What do we earn?'\n            clickFunction={() => setHasBadges(true)}\n          />\n          <Lock\n            answer={'3'}\n            question='How many fingers do we hold up for our sign?'\n            clickFunction={() => setHasSign(true)}\n          />\n          <Lock\n            answer={'be prepared'}\n            question='What our motto?'\n            clickFunction={() => setHasMotto(true)}\n          />\n          <Lock\n            answer={'friendship circle'}\n            question='How do we end our meetings (2 words)?'\n            clickFunction={() => setHasCircle(true)}\n          />\n        </div>\n      }\n      {hasBadges && hasSign && hasMotto && hasCircle &&\n        <Button\n          className='btn'\n          onClick={() => history.push('/christmas')}\n        >\n          Keep Solving\n        </Button>\n      }\n    </div>\n  );\n}\n\n\n","/Users/kellymariewright/Documents/Girl Scouts/troop2256-detective/src/Christmas.js",["39","40","41","42","43","44","45"],{"ruleId":"46","severity":1,"message":"47","line":23,"column":11,"nodeType":"48","endLine":23,"endColumn":40},{"ruleId":"46","severity":1,"message":"47","line":53,"column":11,"nodeType":"48","endLine":53,"endColumn":33},{"ruleId":"46","severity":1,"message":"47","line":61,"column":11,"nodeType":"48","endLine":61,"endColumn":34},{"ruleId":"49","severity":1,"message":"50","line":1,"column":27,"nodeType":"51","messageId":"52","endLine":1,"endColumn":36},{"ruleId":"46","severity":1,"message":"47","line":54,"column":13,"nodeType":"48","endLine":54,"endColumn":52},{"ruleId":"53","severity":1,"message":"54","line":30,"column":9,"nodeType":"48","endLine":37,"endColumn":10},{"ruleId":"49","severity":1,"message":"55","line":3,"column":3,"nodeType":"51","messageId":"52","endLine":3,"endColumn":6},{"ruleId":"49","severity":1,"message":"56","line"